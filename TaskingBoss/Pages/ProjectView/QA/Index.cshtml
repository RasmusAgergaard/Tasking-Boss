@page
@model TaskingBoss.Pages.ProjectView.QA.IndexModel
@{
    ViewData["Title"] = "QA";
    Layout = "_ProjectViewLayout";
}

<nav aria-label="breadcrumb">
    <ol class="breadcrumb px-3 py-2">
        <li class="breadcrumb-item"><a asp-page="/Projects/List">Projects</a></li>
        <li class="breadcrumb-item"><a asp-page="/ProjectView/Index" asp-route-projectId="@Model.Project.ProjectId">This project</a></li>
        <li class="breadcrumb-item active" aria-current="page">QA</li>
    </ol>
</nav>

<div class="w-100 text-center">

    <h3>@ViewData["Title"]</h3>
    <hr />

</div>


<div class="row w-100">

    @foreach (var task in Model.QaTasks)
    {
        <partial name="_QaTaskCard" model="task" />
    }

</div>